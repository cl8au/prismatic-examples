{
  "name": "@prismatic-io/aws-s3",
  "private": true,
  "version": "0.0.1",
  "description": "AWS S3 Component for Prismatic",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint --quiet --ext .ts,.js --ignore-pattern '*testapp*' .",
    "format": "prettier --loglevel error --write './src/**/*.{js,jsx,ts,tsx,css,html,json,scss}' 'documentation/*.mdx'",
    "check": "prettier --check './src/**/*.{js,jsx,ts,tsx,css,html,json,scss}' 'documentation/*.mdx'",
    "build": "webpack",
    "test": "jest --maxWorkers=4",
    "testapp": "node ./test/testapp.js",
    "build:testapp": "yarn run build && yarn run testapp",
    "dev": "nodemon --watch src -e ts,ejs --exec yarn run build:testapp"
  },
  "dependencies": {
    "@prismatic-io/spectral": "^1.0.5",
    "aws-sdk": "^2.670.0"
  },
  "author": "Prismatic",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.17.4",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "ts-jest": "^26.1.1"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "env": {
      "node": true
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "ignorePatterns": [
      "dist",
      "node_modules",
      "webpack.config.js",
      "jest.config.js"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  }
}
